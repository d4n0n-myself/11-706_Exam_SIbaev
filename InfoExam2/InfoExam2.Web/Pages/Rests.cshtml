
@model IEnumerable<InfoExam2.Core.Restaraunt>
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

@foreach (var p in Model)
{
    <div>
        Name: @p.Name <br/>
        <form asp-action="GetDishesFromRestaraunt" asp-controller="Home">
            <input type="hidden" value="@p.Name"/>
            <input type="submit"/>
        </form>
    </div>
}